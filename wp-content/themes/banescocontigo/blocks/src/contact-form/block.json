					},
					"secondLine": {
						"desktop": "",
						"mobile": "",
						"target": ""
					},
					"icon": {
						"desktop": {
							"url": "whatsapp.png",
							"alt": ""
						},
						"mobile": {
							"url": "",
							"alt": ""
						}
					}
				}
			]
		}
	}
}
