					"bottom": "100px"
				},
				"mobile": {
					"top": "50px",
					"bottom": "50px"
				}
			}
		},
		"margins": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "0px",
					"bottom": "0px"
				},
				"mobile": {
					"top": "0px",
					"bottom": "0px"
				}
			}
		},
		"gradient": {
			"type": "string",
			"default": "#FFFFFF"
		},
		"items": {
			"type": "array",
			"default": [
				{
					"name": "tab1",
					"title": "Title #1",
					"target": "/"
				},
				{
					"name": "tab2",
					"title": "Title #2",
					"target": "/"
				},
				{
					"name": "tab3",
					"title": "Title #3",
					"target": "/"
				},
				{
					"name": "tab4",
					"title": "Title #4",
					"target": "/"
				}
			]
