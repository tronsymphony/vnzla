{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "banescocontigo/references",
	"version": "1.0.0",
	"title": "References",
	"category": "widgets",
	"icon": "editor-insertmore",
	"supports": {
		"html": false
	},
	"textdomain": "blocks",
	"editorScript": "file:../../../blocks/build/references.js",
	"editorStyle": "file:../../../blocks/build/references.css",
	"style": "file:../../../blocks/build/style-references.css",
	"attributes": {
		"paddings": {
			"type": "object",
			"default": {
				"desktop": {
