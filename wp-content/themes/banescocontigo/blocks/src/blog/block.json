{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "banescocontigo/blog",
	"version": "1.0.0",
	"title": "Blog",
	"category": "widgets",
	"icon": "warning",
	"supports": {
		"html": false
	},
	"textdomain": "blocks",
	"editorScript": "file:../../../blocks/build/blog.js",
